    "__format": "biome format --write && biome check --write",
    "supabase:db:push": "supabase db push"
  },
  "dependencies": {
    "@mastra/core": "0.24.8",
    "@mastra/libsql": "0.16.4",
    "@mastra/loggers": "0.10.19",
    "@mastra/memory": "0.15.13",
    "@supabase/ssr": "0.8.0",
    "@supabase/supabase-js": "2.89.0",
    "jotai": "2.16.1",
    "mastra": "0.18.8",
    "next": "16.0.10",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "zod": "4.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "4",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "supabase": "2.70.5",
    "tailwindcss": "4",
    "typescript": "5"
  }
